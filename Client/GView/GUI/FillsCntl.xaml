<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:OpenWLS.Client.GView.GUI" x:Class="OpenWLS.Client.GView.GUI.FillsCntl" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="500">
    <Grid>
        <local:VdItemList x:Name="listCntl" HorizontalAlignment="Left"   VerticalAlignment="Stretch" Width="120" SelectionChanged="listCntl_SelectionChanged"/>
        <local:FillCntl x:Name="fillCntl" HorizontalAlignment="Left" Margin="121,1,0,0"  VerticalAlignment="Top" Width="Auto" />
    </Grid>
</UserControl>
